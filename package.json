{
    "name": "redpanda-debezium-mysql-test",
    "version": "0.1.0",
    "description": "redpanda-debezium-mysql-test",
    "main": "dist/tsc/index.js",
    "bin": "dist/index.js",
    "license": "UNLICENSED",
    "private": false,
    "types": "dist/tsc/index.d.ts",
    "files": [
        "/dist"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/jseparovic/redpanda-debezium-mysql-test.git"
    },
    "scripts": {
        "build": "rm -rf dist && tsc -p tsconfig.json"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 7,
            "sourceType": "module"
        },
        "env": {
            "node": true
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    },
    "dependencies": {
        "axios": "^1.2.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/node": "^16.11.26",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^7.22.0",
        "typescript": "^4.6.2"
    },
    "esformatter": {
        "plugins": [
            "esformatter-quotes"
        ],
        "quotes": {
            "type": "single"
        }
    }
}
